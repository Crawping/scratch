<?xml version="1.0" encoding="UTF-8"?>
<Wix xmlns="http://schemas.microsoft.com/wix/2006/wi">
	<Fragment>
    <UI Id="Start_Installation_Dialog_UI">
      <Dialog Id="Start_Installation_Dialog" X="50" Y="50" Width="375" Height="270" Title="[ProductName] Setup" TrackDiskSpace="yes" CustomPalette="yes">
        <Control Id="InstallNow" Type="PushButton" X="244" Y="244" Width="56" Height="17" Text="&amp;Next &gt;" TabSkip="no" Default="yes">
          <Publish Event="SpawnWaitDialog" Value="WaitForCosting">CostingComplete = 1</Publish>
          <Publish Event="SpawnDialog" Value="OutOfDisk">OutOfDiskSpace = 1</Publish>
          <Publish Event="Reinstall" Value="ALL">Installed AND MaintenanceMode="Repair"</Publish>
          <Publish Event="EndDialog" Value="Return"><![CDATA[OutOfDiskSpace<>1]]></Publish>
        </Control>
        <Control Id="Previous" Type="PushButton" X="189" Y="244" Width="56" Height="17" Text="&lt; &amp;Back" TabSkip="no">
          <Publish Event="NewDialog" Value="Installation_Type_Dialog"><![CDATA[0 AND (InstallMode<>"Custom" AND NOT Installed AND NOT RESUME AND NOT Preselected)]]></Publish>
          <Publish Event="NewDialog" Value="License_Dialog">0 AND (NOT Installed AND RESUME)</Publish>
          <Publish Event="NewDialog" Value="Palm_User_Information_Dialog">0 AND (Installed AND MaintenanceMode="Modify" AND PALMUSERS = 1)</Publish>
          <Publish Event="NewDialog" Value="Palm_User_Information_Dialog">0 AND (InstallMode  = "Custom" AND NOT Installed AND NOT RESUME AND NOT Preselected AND PALMUSERS = 1)</Publish>
          <Publish Event="NewDialog" Value="Palm_User_Information_Dialog">Installed AND MaintenanceMode="Modify" AND PALMUSERS = 1</Publish>
          <Publish Event="NewDialog" Value="Palm_User_Information_Dialog">InstallMode  = "Custom" AND NOT Installed AND NOT RESUME AND NOT Preselected AND PALMUSERS = 1</Publish>
          <Publish Event="NewDialog" Value="Select_Feature_Dialog">0 AND (Installed AND MaintenanceMode="Modify")</Publish>
          <Publish Event="NewDialog" Value="Select_Feature_Dialog">0 AND (InstallMode  = "Custom" AND NOT Installed AND NOT RESUME AND NOT Preselected)</Publish>
          <Publish Event="NewDialog" Value="Select_Feature_Dialog">Installed AND MaintenanceMode="Modify"</Publish>
          <Publish Event="NewDialog" Value="License_Dialog">NOT Installed AND RESUME</Publish>
          <Publish Event="NewDialog" Value="Single_Feature_Destination"><![CDATA[InstallMode<>"Custom" AND NOT Installed AND NOT RESUME AND NOT Preselected]]></Publish>
          <Publish Event="NewDialog" Value="Single_Feature_Destination">InstallMode = "Custom" AND NOT Installed AND NOT RESUME AND NOT Preselected AND NOT Installed</Publish>
          <Publish Event="NewDialog" Value="User_Information_Dialog">NOT Installed AND NOT RESUME AND Preselected</Publish>
          <Publish Event="NewDialog" Value="User_Information_Dialog">0 AND (NOT Installed AND NOT RESUME AND Preselected)</Publish>
          <Publish Event="NewDialog" Value="Maintenance_Welcome_Dialog">0 AND (Installed AND MaintenanceMode = "Repair")</Publish>
          <Publish Event="NewDialog" Value="Maintenance_Welcome_Dialog">Installed AND MaintenanceMode = "Repair"</Publish>
        </Control>
        <Control Id="Cancel" Type="PushButton" X="309" Y="244" Width="56" Height="17" Text="Cancel" TabSkip="no" Cancel="yes">
          <Publish Event="SpawnDialog" Value="Cancel">1</Publish>
        </Control>
        <Control Id="Text51" Type="Text" X="30" Y="65" Width="290" Height="26" Text="Click the Back button to reenter the installation information or click Cancel to exit the wizard." TabSkip="yes" />
        <Control Id="Line5" Type="Line" X="94" Y="234" Width="281" Height="1" TabSkip="yes" Disabled="yes" />
        <Control Id="Bitmap29" Type="Bitmap" X="0" Y="0" Width="375" Height="44" Text="top16" TabSkip="yes" Disabled="yes" />
        <Control Id="Line29" Type="Line" X="0" Y="44" Width="375" Height="1" TabSkip="yes" Disabled="yes" />
        <Control Id="Text50" Type="Text" X="2" Y="230" Width="92" Height="12" Text="{\MS_Sans_Serif_80}[GFITM]" TabSkip="yes" />
        <Control Id="Text26" Type="Text" X="10" Y="8" Width="310" Height="12" Text="[DIALOG_TITLE_FONT_DEFAULT]Ready to Modify the Application" TabSkip="yes" Transparent="yes">
          <Condition Action="hide"><![CDATA[Not Installed OR MaintenanceMode<>"Modify"]]></Condition>
        </Control>
        <Control Id="Text244" Type="Text" X="20" Y="22" Width="290" Height="24" Text="Click Next to begin installation." TabSkip="yes" Transparent="yes" />
        <Control Id="Text2512" Type="Text" X="10" Y="8" Width="310" Height="12" Text="[DIALOG_TITLE_FONT_DEFAULT]Ready to Repair the Application" TabSkip="yes" Transparent="yes">
          <Condition Action="hide"><![CDATA[Not Installed OR MaintenanceMode<>"Repair" OR RESUME]]></Condition>
        </Control>
        <Control Id="Text251" Type="Text" X="10" Y="8" Width="310" Height="12" Text="[DIALOG_TITLE_FONT_DEFAULT]Ready to Install the Application" TabSkip="yes" Transparent="yes">
          <Condition Action="hide">Installed OR RESUME</Condition>
        </Control>
        <Control Id="Text2513" Type="Text" X="10" Y="8" Width="310" Height="12" Text="[DIALOG_TITLE_FONT_DEFAULT]Ready to Resume [ProductName] installation" TabSkip="yes" Transparent="yes">
          <Condition Action="hide">NOT RESUME</Condition>
        </Control>
      </Dialog>
    </UI>
	</Fragment>
</Wix>