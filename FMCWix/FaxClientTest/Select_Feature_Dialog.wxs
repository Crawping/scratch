<?xml version="1.0" encoding="UTF-8"?>
<Wix xmlns="http://schemas.microsoft.com/wix/2006/wi">
	<Fragment>
    <UI Id="Select_Feature_Dialog_UI">
      <Dialog Id="Select_Feature_Dialog" X="50" Y="50" Width="375" Height="270" Title="[ProductName] Setup" TrackDiskSpace="yes" CustomPalette="yes">
        <Control Id="Tree" Type="SelectionTree" X="5" Y="55" Width="174" Height="133" Property="_BrowseProperty" TabSkip="no" />
        <Control Id="Browse" Type="PushButton" X="300" Y="204" Width="56" Height="17" Text="Br&amp;owse" TabSkip="no">
          <Condition Action="hide">Installed</Condition>
          <Publish Event="SelectionBrowse" Value="Browse">1</Publish>
        </Control>
        <Control Id="Next" Type="PushButton" X="244" Y="244" Width="56" Height="17" Text="&amp;Next &gt;" TabSkip="no" Default="yes">
          <Publish Event="SpawnWaitDialog" Value="WaitForCosting">CostingComplete = 1</Publish>
          <Publish Event="SpawnDialog" Value="OutOfDisk">OutOfDiskSpace = 1</Publish>
          <Publish Event="NewDialog" Value="Palm_User_Information_Dialog">PALMUSERS = 1</Publish>
          <Publish Event="NewDialog" Value="Start_Installation_Dialog"><![CDATA[OutOfDiskSpace<>1]]></Publish>
          <Subscribe Event="SelectionNoItems" Attribute="Enabled" />
        </Control>
        <Control Id="Previous" Type="PushButton" X="189" Y="244" Width="56" Height="17" Text="&lt; &amp;Back" TabSkip="no">
          <Publish Event="NewDialog" Value="Installation_Type_Dialog">0 AND (NOT Installed)</Publish>
          <Publish Event="NewDialog" Value="Maintenance_Welcome_Dialog">0 AND (Installed)</Publish>
          <Publish Event="NewDialog" Value="Maintenance_Welcome_Dialog">Installed</Publish>
          <Publish Event="NewDialog" Value="Single_Feature_Destination">NOT Installed</Publish>
        </Control>
        <Control Id="Reset" Type="PushButton" X="124" Y="244" Width="56" Height="17" Text="&amp;Reset" TabSkip="no">
          <Publish Event="Reset" Value="0">1</Publish>
          <Subscribe Event="SelectionNoItems" Attribute="Enabled" />
        </Control>
        <Control Id="DiskCost" Type="PushButton" X="59" Y="244" Width="56" Height="17" Text="&amp;Disk Cost" TabSkip="no">
          <Publish Event="SpawnWaitDialog" Value="WaitForCosting">CostingComplete = 1</Publish>
          <Publish Event="SpawnDialog" Value="DiskCost">1</Publish>
          <Subscribe Event="SelectionNoItems" Attribute="Enabled" />
        </Control>
        <Control Id="Cancel" Type="PushButton" X="309" Y="244" Width="56" Height="17" Text="Cancel" TabSkip="no" Cancel="yes">
          <Publish Event="SpawnDialog" Value="Cancel">1</Publish>
        </Control>
        <Control Id="Size" Type="Text" X="191" Y="143" Width="167" Height="40" Text="The size of the current selection xxx xxx xxxx xxxx xxxx xxxx xxx xxxx xxxx xxxx xxxx xxx" TabSkip="yes">
          <Subscribe Event="SelectionSize" Attribute="Text" />
        </Control>
        <Control Id="Line21" Type="Line" X="0" Y="50" Width="375" Height="1" TabSkip="yes" Disabled="yes" />
        <Control Id="ActionDescription" Type="Text" X="191" Y="120" Width="166" Height="26" Text="Description of the currently selected action xxx xxx xxx xxxx xxxx xxxx xxxx xxxx" TabSkip="yes">
          <Subscribe Event="SelectionAction" Attribute="Text" />
        </Control>
        <Control Id="ItemDescription" Type="Text" X="191" Y="72" Width="166" Height="45" Text="Multiline description of the currently selected item xxx xxxx xxxx xxxx xxxx  xxxxx xxx xxxx xxxx xxxx xxx xxxx xx xxxxx xxxxx x" TabSkip="yes">
          <Subscribe Event="SelectionDescription" Attribute="Text" />
        </Control>
        <Control Id="LocationFrame" Type="GroupBox" X="5" Y="190" Width="362" Height="38" TabSkip="yes">
          <Condition Action="hide">Installed</Condition>
        </Control>
        <Control Id="LocationPath" Type="Text" X="11" Y="213" Width="283" Height="11" Text="Path" TabSkip="yes">
          <Condition Action="hide">Installed</Condition>
          <Subscribe Event="SelectionPath" Attribute="Text" />
        </Control>
        <Control Id="LocationText" Type="Text" X="11" Y="200" Width="261" Height="11" Text="Current location:" TabSkip="yes">
          <Condition Action="hide">Installed</Condition>
          <Subscribe Event="SelectionPathOn" Attribute="Visible" />
        </Control>
        <Control Id="SelectionFrame" Type="GroupBox" X="186" Y="51" Width="180" Height="140" TabSkip="yes" />
        <Control Id="Bitmap28" Type="Bitmap" X="0" Y="0" Width="375" Height="50" Text="top16" TabSkip="yes" Disabled="yes" />
        <Control Id="Line22" Type="Line" X="94" Y="234" Width="281" Height="1" TabSkip="yes" Disabled="yes" />
        <Control Id="Text24" Type="Text" X="20" Y="22" Width="290" Height="24" Text="Please select which features you would like to install." TabSkip="yes" Transparent="yes" />
        <Control Id="Text49" Type="Text" X="2" Y="230" Width="92" Height="12" Text="[GFITM]" TabSkip="yes" />
        <Control Id="Text23" Type="Text" X="10" Y="8" Width="310" Height="12" Text="[DIALOG_TITLE_FONT_DEFAULT]Select Features" TabSkip="yes" Transparent="yes" />
        <Control Id="Text41" Type="Text" X="191" Y="60" Width="115" Height="14" Text="[DIALOG_TITLE_FONT_DEFAULT]Feature Description:" TabSkip="yes" />
      </Dialog>
    </UI>
	</Fragment>
</Wix>