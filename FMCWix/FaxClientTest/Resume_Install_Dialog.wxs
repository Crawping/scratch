<?xml version="1.0" encoding="UTF-8"?>
<Wix xmlns="http://schemas.microsoft.com/wix/2006/wi">
	<Fragment>
    <UI Id="Resume_Install_Dialog">
      <Dialog Id="Resume_Install_Dialog" X="50" Y="50" Width="375" Height="270" Title="[ProductName] Setup" CustomPalette="yes">
        <Control Id="InstallNow" Type="PushButton" X="244" Y="244" Width="56" Height="17" Text="&amp;Next &gt;" TabSkip="no" Default="yes">
          <Publish Event="EndDialog" Value="Return"><![CDATA[OutOfDiskSpace<>1]]></Publish>
          <Publish Event="SpawnDialog" Value="OutOfDisk">OutOfDiskSpace = 1</Publish>
        </Control>
        <Control Id="Cancel" Type="PushButton" X="309" Y="244" Width="56" Height="17" Text="Cancel" TabSkip="no" Cancel="yes">
          <Publish Event="SpawnDialog" Value="Cancel">1</Publish>
        </Control>
        <Control Id="Text" Type="Text" X="136" Y="55" Width="227" Height="42" Text="Are you ready to have the Installation Wizard continue the installation?" TabSkip="yes" Transparent="yes">
          <Condition Action="show">NOT RESUME</Condition>
          <Condition Action="hide">RESUME</Condition>
        </Control>
        <Control Id="Text2" Type="Text" X="136" Y="55" Width="227" Height="55" Text="Are you ready to have the Installation Wizard complete your suspended installation?" TabSkip="yes" Transparent="yes">
          <Condition Action="show">RESUME</Condition>
          <Condition Action="hide">NOT RESUME</Condition>
        </Control>
        <Control Id="Text31" Type="Text" X="136" Y="6" Width="227" Height="49" Text="{&amp;Arial14}Resuming the [ProductName] Installation Wizard" TabSkip="yes" Transparent="yes" />
        <Control Id="Bitmap32" Type="Bitmap" X="0" Y="0" Width="375" Height="234" Text="side16" TabSkip="yes" Disabled="yes" />
        <Control Id="Line7" Type="Line" X="0" Y="234" Width="375" Height="1" TabSkip="yes" Disabled="yes" />
      </Dialog>
    </UI>
	</Fragment>
</Wix>