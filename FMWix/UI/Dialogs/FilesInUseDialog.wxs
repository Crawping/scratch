<?xml version="1.0" encoding="UTF-8"?>
<Wix xmlns="http://schemas.microsoft.com/wix/2006/wi">
	<Fragment>
    <UI>
      <Dialog Id="FilesInUse" X="50" Y="50" Width="375" Height="270" Title="!(loc.IDS_DIALOGTITLE)" KeepModeless="yes">
        <Control Id="Retry" Type="PushButton" X="304" Y="243" Width="56" Height="17" Text="[DefDialogFont]&amp;Retry" TabSkip="no" Default="yes">
          <Publish Event="EndDialog" Value="Retry">1</Publish>
        </Control>
        <Control Id="Ignore" Type="PushButton" X="235" Y="243" Width="56" Height="17" Text="[DefDialogFont]&amp;Ignore" TabSkip="no">
          <Publish Event="EndDialog" Value="Ignore">1</Publish>
        </Control>
        <Control Id="Text" Type="Text" X="20" Y="22" Width="290" Height="24" Text="[DefDialogFont]Files that need to be updated are in use." TabSkip="no" Transparent="yes" />
        <Control Id="List" Type="ListBox" X="19" Y="87" Width="336" Height="141" Property="FileInUseProcess" Text="[DefDialogFont]" TabSkip="no" />
        <Control Id="Exit" Type="PushButton" X="166" Y="243" Width="56" Height="17" Text="[DefDialogFont]&amp;Exit" TabSkip="no" Cancel="yes">
          <Publish Event="EndDialog" Value="Exit">1</Publish>
        </Control>
        <Control Id="Text54" Type="Text" X="2" Y="230" Width="74" Height="12" Text="[DefDialogFont][GFITM]" TabSkip="yes" />
        <Control Id="Text60" Type="Text" X="20" Y="55" Width="330" Height="30" Text="Listed below are busy files followed by the application that's using the file. Close the application and click Retry, or click Ignore to replace the files after a reboot. Clicking Exit quits the installation." TabSkip="yes" />
        <Control Id="Text58" Type="Text" X="10" Y="8" Width="310" Height="12" Text="[_WiseDialogTitleFontDefault]Files In Use" TabSkip="yes" Transparent="yes" />
        <Control Id="Bitmap62" Type="Bitmap" X="0" Y="0" Width="375" Height="50" Text="top16" TabSkip="yes" Disabled="yes" />
        <Control Id="Line45" Type="Line" X="0" Y="50" Width="375" Height="1" TabSkip="yes" Disabled="yes" />
        <Control Id="Line59" Type="Line" X="82" Y="234" Width="277" Height="0" TabSkip="yes" Disabled="yes" />
      </Dialog>
    </UI>
	</Fragment>
</Wix>